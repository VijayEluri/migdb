import builder_rdb_link;
modeltype RDB uses "http://www.collectionspro.eu/jam/mm/rdb";
transformation addsequence(out inoutModel : RDB);


main() {
	log("TEST_RDB");
	log("op: ADDSEQUENCE");	
	
	var model : RDB::ModelRoot := object ModelRoot {
		modelGenerations += object ModelGeneration {
			isMissing := false;
			schemas := Set{_schema("public")};
		};
	};
	
	model.operations += _addSequence("public", "seq_global", 1);
	 
}