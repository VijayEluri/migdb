modeltype RDB uses "http://www.collectionspro.eu/jam/mm/rdb";

transformation build_model(inout inoutModel : RDB);


main() {		
			
	var schema : RDB::operations::AddSchema	:= object RDB::operations::AddSchema {
		name := "public";
	};
	
	var table : RDB::operations::AddTable := object RDB::operations::AddTable {
		owningSchemaName := "public";
		name := "first";
	};
	
	var column : RDB::operations::AddColumn := object RDB::operations::AddColumn{
		owningSchemaName := "public";
		owningTableName := "first";
		name := "Name";
		type := RDB::PrimitiveType::char;
	};
	
	var index : RDB::operations::AddIndex := object RDB::operations::AddIndex{
		owningSchemaName := "public";
		owningTableName := "first";
		name := "prvniIndex";
		columnsNames := OrderedSet{"Name"};
	};	  									  												  												  									  	

	var model : RDB::ModelRoot := object ModelRoot {
		modelGenerations += object ModelGeneration {
			isMissing := false;
		};
	 };
	 
	 model.operations += schema;
	 model.operations += table;
	 model.operations += column;
	 model.operations += index;

}