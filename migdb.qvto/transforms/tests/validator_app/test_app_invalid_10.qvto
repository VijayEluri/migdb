import builder_app;
modeltype APP uses "http://www.collectionspro.eu/jam/mm/app";
transformation secondAPP(out inoutModel : APP);

main() {
	log("TEST_APP_INVALID_10 - invalid : nonInteger ID ");	
	var integer : APP::PrimitiveClass := _primitiveClass("Integer", PrimitiveType::int);
	var string : APP::PrimitiveClass := _primitiveClass("String", PrimitiveType::char);
	
	var person : APP::Class := _class("Person", OrderedSet{ _property("name", string), _property("id", integer, true)});
	var thing : APP::Class := _class("Thing", OrderedSet{ _property("name", string), _property("id", string, true)});	
	
	var model : APP::ModelRoot := object ModelRoot {
		modelGenerations += object ModelGeneration {
			isMissing := false;
			classes += Set{integer,string,person, thing
			};
		};
	 };
	 
}