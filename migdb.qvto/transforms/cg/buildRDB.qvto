import builder_rdb_link;
modeltype RDB uses "http://www.collectionspro.eu/jam/mm/rdb";
transformation buildRDB(inout inoutModel : RDB);

main() {

	var model : RDB::Operations := object Operations {
		operations := OrderedSet{}; 
	};
	
	model.operations += _addSchema("public");
	model.operations += _addSequence("public", "seq_global", 1); 
}