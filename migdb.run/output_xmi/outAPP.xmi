<?xml version="1.0" encoding="ASCII"?>
<app:ModelRoot xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:app="http://www.collectionspro.eu/jam/mm/app" xmlns:app-ops="http://www.collectionspro.eu/jam/mm/app-ops" xsi:schemaLocation="http://www.collectionspro.eu/jam/mm/app java://mm.app.AppPackage http://www.collectionspro.eu/jam/mm/app-ops java://mm.app.operations.OperationsPackage">
  <modelGenerations>
    <classes xsi:type="app:Class" name="NaturalPerson">
      <properties name="name"/>
      <properties name="surname"/>
      <properties name="country"/>
      <properties name="id" type="//@modelGenerations.0/@classes.3" isID="true"/>
      <properties name="street"/>
      <properties name="zip"/>
      <properties name="city"/>
    </classes>
    <classes xsi:type="app:Class" name="LegalPerson">
      <properties name="city"/>
      <properties name="street"/>
      <properties name="regNo"/>
      <properties name="zip"/>
      <properties name="busName"/>
      <properties name="id" type="//@modelGenerations.0/@classes.3" isID="true"/>
      <properties name="country"/>
    </classes>
    <classes xsi:type="app:Class" name="Country">
      <properties name="id" type="//@modelGenerations.0/@classes.3" isID="true"/>
      <properties name="name"/>
    </classes>
    <classes xsi:type="app:PrimitiveClass" name="Integer" primitiveType="int"/>
    <classes xsi:type="app:PrimitiveClass" name="String" primitiveType="char"/>
  </modelGenerations>
  <operations xsi:type="app-ops:AddClass" name="Party"/>
  <operations xsi:type="app-ops:AddProperty" owningClassName="Party" name="street" type="String"/>
  <operations xsi:type="app-ops:AddProperty" owningClassName="Party" name="city" type="String"/>
  <operations xsi:type="app-ops:AddProperty" owningClassName="Party" name="zip" type="String"/>
  <operations xsi:type="app-ops:AddProperty" owningClassName="Party" name="country" type="Country"/>
  <operations xsi:type="app-ops:SetParent" name="LegalPerson" parentName="Party">
    <properties>street</properties>
    <properties>city</properties>
    <properties>zip</properties>
    <properties>country</properties>
  </operations>
  <operations xsi:type="app-ops:SetParent" name="NaturalPerson" parentName="Party">
    <properties>street</properties>
    <properties>city</properties>
    <properties>zip</properties>
    <properties>country</properties>
  </operations>
  <operations xsi:type="app-ops:ExtractClass" sourceClassName="Party" extractClassName="Address">
    <extractPropertiesNames>street</extractPropertiesNames>
    <extractPropertiesNames>city</extractPropertiesNames>
    <extractPropertiesNames>zip</extractPropertiesNames>
    <extractPropertiesNames>country</extractPropertiesNames>
  </operations>
  <operations xsi:type="app-ops:RenameProperty" owningClassName="Party" name="address" newName="residentalAddress"/>
  <operations xsi:type="app-ops:AddProperty" owningClassName="Party" name="contactAddress" type="Country"/>
