import builder_rdb_link;
modeltype RDB uses "http://www.collectionspro.eu/jam/mm/rdb";
transformation addtable(out inoutModel : RDB);


main() {
	log("TEST_RDB");
	log("op: ADDTABLE");	
	
	var model : RDB::ModelRoot := object ModelRoot {
		modelGenerations += object ModelGeneration {
			isMissing := false;
			schemas := Set{_schema("public", _sequence("seq_global", 1))};
		};
	};
	
	model.operations += _addTable("public", "first");
	 
}